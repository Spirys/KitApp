doctype html
html(lang='en')
    head
        meta(charset='utf-8')
        link(rel='apple-touch-icon', sizes='76x76', href='../images/apple-icon.png')
        link(rel='icon', type='image/png', href='../images/favicon.png')
        meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
        title Dashboard
        meta(content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0', name='viewport')
        meta(name='viewport', content='width=device-width')
        link(href='../stylesheets/bootstrap.min.css', rel='stylesheet')
        link(href='../stylesheets/material-dashboard.css?v=1.2.0', rel='stylesheet')
        link(href='../stylesheets/demo.css', rel='stylesheet')
        link(href='https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css', rel='stylesheet')
        link(href='https://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons', rel='stylesheet', type='text/css')
    body
        .wrapper
            .sidebar(data-color='purple', data-image='../images/f.jpg')
                .logo
                    a.simple-text(href='https://kitapptatar.ru')
                        | Kitapp
                .sidebar-wrapper
                    ul.nav
                        li.active
                            a(href='https://kitapptatar.ru/dashboard')
                                i.material-icons dashboard
                                p Dashboard
                        li
                            a(href='./user')
                                i.material-icons person
                                p User Profile
                        li
                            a(href='./card')
                                i.material-icons description
                                p Library card
                        li
                            a(href='./search')
                                i.material-icons book
                                p Search books
            .main-panel
                nav.navbar.navbar-transparent.navbar-absolute
                    .container-fluid
                        .navbar-header
                            button.navbar-toggle(type='button', data-toggle='collapse')
                                span.sr-only Toggle navigation
                                span.icon-bar
                                span.icon-bar
                                span.icon-bar
                            a.navbar-brand(href='#')  Dashboard
                        .collapse.navbar-collapse
                            ul.nav.navbar-nav.navbar-right
                                li
                                    a.dropdown-toggle(href='#pablo', data-toggle='dropdown')
                                        i.material-icons dashboard
                                        p.hidden-lg.hidden-md Dashboard
                                li.dropdown
                                    a.dropdown-toggle(href='#', data-toggle='dropdown')
                                        i.material-icons notifications
                                        span.notification 5
                                        p.hidden-lg.hidden-md Notifications
                                    ul.dropdown-menu
                                        // Just examples
                                        li
                                            a(href='#') New book is available
                                        li
                                            a(href='#') You have 3 books on hands
                                        li
                                            a(href='#') You have 4 books that are due
                                        li
                                            a(href='#') A book has been returned
                                        li
                                            a(href='#') The book is now available
                                li
                                    a.dropdown-toggle(href='#pablo', data-toggle='dropdown')
                                        i.material-icons settings
                                        p.hidden-lg.hidden-md Profile
                            form.navbar-form.navbar-right(role='search')
                                .form-group.is-empty
                                    input.form-control(type='text', placeholder='Search')
                                    span.material-input
                                button.btn.btn-white.btn-round.btn-just-icon(type='submit')
                                    i.material-icons search
                                    .ripple-container
                .content
                    .container-fluid
                        .row
                            .col-lg-3.col-md-6.col-sm-6
                                .card.card-stats
                                    .card-header(data-background-color='orange')
                                        i.material-icons perm_scan_wifi
                                    .card-content
                                        p.category Fines
                                        h3.title
                                            | 69
                                            small RUB
                                    .card-footer
                                        .stats
                                            i.material-icons.text-danger warning
                                            a(href='#pablo') Pay fines
                            .col-lg-3.col-md-6.col-sm-6
                                .card.card-stats
                                    .card-header(data-background-color='green')
                                        i.material-icons book
                                    .card-content
                                        p.category Books on hands
                                        h3.title 6
                                    .card-footer
                                        .stats
                                            i.material-icons date_range
                                            |  Last 24 Hours
                            .col-lg-3.col-md-6.col-sm-6
                                .card.card-stats
                                    .card-header(data-background-color='red')
                                        i.material-icons info_outline
                                    .card-content
                                        p.category Upcoming deadlines
                                        h3.title 32
                                    .card-footer
                                        .stats
                                            i.material-icons local_offer
                                            a(href='#pablo')  See library working hours
                            .col-lg-3.col-md-6.col-sm-6
                                .card.card-stats
                                    .card-header(data-background-color='blue')
                                        i.material-icons bookmark
                                    .card-content
                                        p.category Books in the queue
                                        h3.title 23
                                    .card-footer
                                        .stats
                                            i.material-icons update
                                            |  Just Updated
                footer.footer
                    .container-fluid
                        nav.pull-left
                            ul
                                li
                                    a(href='#')
                                        | Kitapp
                                li
                                    a(href='#')
                                        | About Innopolis University
                                li
                                    a(href='#')
                                        | Library Rules
                                li
                                    a(href='#')
                                        | Feedback
                        p.copyright.pull-right
                            | Â©
                            script.
                                document.write(new Date().getFullYear())
                            a(href='https://kitapptatar.com') Kitapp
                            | , made with
                            i.fa.fa-heart.heart
                            |  for Innopolis University
    script(src='../javascripts/jquery-3.2.1.min.js', type='text/javascript')
    script(src='../javascripts/bootstrap.min.js', type='text/javascript')
    script(src='../javascripts/material.min.js', type='text/javascript')
    script(src='../javascripts/chartist.min.js')
    script(src='../javascripts/arrive.min.js')
    script(src='../javascripts/perfect-scrollbar.jquery.min.js')
    script(src='../javascripts/bootstrap-notify.js')
    script(type='text/javascript', src='https://maps.googleapis.com/maps/api/js?key=YOUR_KEY_HERE')
    script(src='../javascripts/material-dashboard.js?v=1.2.0')
    script(src='../javascripts/demo.js')
    script(type='text/javascript').
        $(document).ready(function () {
            demo.initDashboardPageCharts();
        });
